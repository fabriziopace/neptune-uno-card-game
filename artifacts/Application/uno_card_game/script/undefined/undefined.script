html {
  height: 100%;
}

#body,
.pageStyle {
  background-color: #262626;
}

.formCreateRoom .sapMDialogScrollCont {
  padding: 0;
}

.listUnoCalled .sapMListHdrText:before {
  content: "UNO!";
  font-weight: bold;
  border-radius: 5rem;
  margin-right: 0.5rem;
  background: #e74c3c;
  padding-left: 0.5rem;
  padding-right: 0.5rem;
}

.listCardsCurrentTurn .sapMListHdrText:after {
  content: "\f521";
  font-family: "fa-solid";
  color: #f6b221;
}

.listCardsCurrentTurn .sapMListHdrText {
  color: #f6b221;
}

.listMyCards.listCardsCurrentTurn .sapMListHdrText:after {
  margin-left: 1rem;
  margin-right: 1rem;
}

.vboxGameTable {
  background: #262626;
  border: 1rem solid #262626;
  width: 100%;
  height: 100%;
}
.vboxGameTable .sapMFlexItem:has(.listMyCards) {
  height: 32%;
  align-content: center;
  margin-bottom: 1rem;
}
.vboxGameTable .vboxCenterGameTable {
  margin-top: 1rem;
  margin-bottom: 1rem;
  justify-content: center;
}
.vboxGameTable .hboxCenterGameTable {
  justify-content: center;
}
.vboxGameTable .hboxCenterGameTable .sapMFlexItem:has(.listCardDeck) {
  height: 100%;
}
.vboxGameTable .vboxCard .sapMFlexItem:has(.imageBackCardBody) {
  z-index: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 100%;
}

.listMyCards,
.listMyCards ul,
.listCardDeck,
.listCardDeck ul {
  height: 7rem;
  display: inline-flex;
  justify-content: center;
}

.listCardDeck li:not(.listCardDeck li:first-child) {
  margin-left: -5.005rem;
}

.listMyCards li:not(.listMyCards li:first-child) {
  margin-left: -4rem;
}

.listMyCards li,
.listCardDeck li {
  width: 5rem;
}
.listMyCards .sapMLIB,
.listCardDeck .sapMLIB {
  background: transparent;
  border-bottom: none;
}

.listMyCards .sapMListHdrText {
  padding-left: 0px;
  padding-right: 0px;
  position: absolute;
  bottom: -2rem;
  top: unset;
  left: 0;
  width: 100%;
  z-index: 100;
  height: 20px;
  line-height: 1.4;
  font-size: 1rem;
  background: transparent;
  border: none;
  color: white;
  font-weight: normal;
  text-align: left;
}

.listOppCards .sapMListHdrText {
  top: -2rem;
  bottom: unset;
  text-align: right;
}

