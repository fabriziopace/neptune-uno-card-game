.vboxCard {
  height: calc(100% - 1px);
  width: calc(100% - 1px);
  border-radius: 0.9rem;
  border: 0.5rem solid #fff;
  box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 0px 1px;
}
.vboxCard:after {
  content: " ";
  position: absolute;
  border-radius: 0.5rem;
  width: calc(100% - 0.8rem);
  height: calc(100% - 0.8rem);
  left: 0.4rem;
  top: 0.4rem;
}
.vboxCard .sapMFlexItem:has(.textCardHeader),
.vboxCard .sapMFlexItem:has(.textCardFooter) {
  height: 1rem;
}
.vboxCard .sapMFlexItem:has(.textCardBody) {
  height: 100%;
}

.textCardStyle {
  z-index: 3;
  color: #fff;
  position: relative;
  font-weight: bold;
  font-style: italic;
}

.textCardHeader,
.textCardFooter {
  color: #fff !important;
  font-size: 1rem;
}

.textCardBody {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 100%;
  transform: skew(-25deg, 0deg);
  font-style: normal;
  width: calc(100% - 1rem);
  font-size: 2rem;
  margin-left: 0.5rem;
  margin-right: 0.5rem;
}

.textCardFooter {
  display: flex;
  justify-content: left;
  rotate: 180deg;
}

.customListCardItem {
  top: 0;
  transition: top 0.5s;
}
.customListCardItem .sapMLIBContent {
  height: 100%;
}
.customListCardItem:hover {
  background: transparent;
  top: -1.5rem;
}
.customListCardItem:focus {
  outline: none;
}
.customListCardItem .sapMLIBActive {
  background: transparent;
}
.customListCardItem.sapMLIBSelected {
  top: -1.5rem;
}
.customListCardItem.sapMLIBHoverable.sapMLIBSelected:hover {
  background-color: transparent;
}

.customListMyCardsItem.sapMLIBTypeInactive:hover,
.customListMyCardsItem.sapMLIBTypeInactive.sapMLIBSelected {
  top: 0 !important;
}

.customListOppCardsItem:hover, .customListOppCardsItem.sapMLIBSelected {
  top: 0 !important;
}

.listMyCards .sapMLIBTypeInactive .textCardHeader,
.listMyCards .sapMLIBTypeInactive .textCardFooter {
  color: #777b7f !important;
}
.listMyCards .sapMLIBTypeInactive .vboxCard {
  border-color: #777b7f;
}
.listMyCards .sapMLIBTypeInactive [data-color=red].vboxCard:after {
  background-color: #6d3331;
}
.listMyCards .sapMLIBTypeInactive [data-color=red].vboxCard .textCardBody {
  background-color: #777b7f;
  color: #6d3331;
}
.listMyCards .sapMLIBTypeInactive [data-color=blue].vboxCard:after {
  background-color: #265271;
}
.listMyCards .sapMLIBTypeInactive [data-color=blue].vboxCard .textCardBody {
  background-color: #777b7f;
  color: #265271;
}
.listMyCards .sapMLIBTypeInactive [data-color=green].vboxCard:after {
  background-color: #215b3f;
}
.listMyCards .sapMLIBTypeInactive [data-color=green].vboxCard .textCardBody {
  background-color: #777b7f;
  color: #215b3f;
}
.listMyCards .sapMLIBTypeInactive [data-color=yellow].vboxCard:after {
  background-color: #71631f;
}
.listMyCards .sapMLIBTypeInactive [data-color=yellow].vboxCard .textCardBody {
  background-color: #777b7f;
  color: #71631f;
}
.listMyCards .sapMLIBTypeInactive [data-color=black].vboxCard:after {
  background-color: #35393d;
}
.listMyCards .sapMLIBTypeInactive [data-color=black].vboxCard .textCardBody {
  background-color: #777b7f;
}
.listMyCards .sapMLIBTypeInactive [data-value=wild].textCardStyle:after,
.listMyCards .sapMLIBTypeInactive [data-value=wilddrawfour].textCardBody:after {
  background: linear-gradient(to right, #6d3331 0%, #6d3331 50%, #265271 50%, #265271 100%), linear-gradient(to right, #71631f 0%, #71631f 50%, #215b3f 50%, #215b3f 100%);
  background-size: 100% 50%;
  background-position: center top, center bottom;
  background-repeat: no-repeat;
  -webkit-background-clip: text;
  background-clip: text;
}

.listOppCards .sapMLIBTypeInactive .vboxCard {
  border-color: #fff;
}

